{
  "name": "framelit",
  "version": "0.1.0",
  "description": "A starter kit for asset management and processing.",
  "dependencies": {
    "less": "~2.1.1",
    "grunt": "~0.4.5",
    "grunt-cli": "~0.1.13",
    "matchdep": "~0.3.0",
    "grunt-closure-tools": "~0.9.7",
    "grunt-contrib-less": "~0.12.0",
    "grunt-contrib-copy": "~0.7.0",
    "grunt-contrib-concat": "~0.5.0",
    "grunt-contrib-clean": "~0.6.0",
    "grunt-contrib-watch": "~0.6.1",
    "underscore": "~1.7.0",
    "jshint": "~2.5.10",
    "time-grunt": "~1.0.0",
    "express": "~4.10.4",
    "ejs": "~1.0.0",
    "debug": "~2.1.0",
    "morgan": "~1.5.0",
    "cookie-parser": "~1.3.3",
    "body-parser": "~1.9.3",
    "karma": "~0.12.28",
    "karma-ng-html2js-preprocessor": "~0.1.2",
    "karma-junit-reporter": "~0.2.2",
    "phantomjs": "~1.9.12",
    "karma-coverage": "~0.2.7",
    "serve-favicon": "~2.1.7"
  },
  "engines": {
    "node": ">= 0.10.2"
  },
  "scripts": {
    "build": "node_modules/grunt-cli/bin/grunt",
    "dev": "node_modules/grunt-cli/bin/grunt dev",
    "cleaninstall": "rm -rf node_modules/; npm install --production;",
    "start": "node ./bootstrap_www",
    "removeexamples": "find -E . -regex '.*example\\.(js|css|less)' -delete",
    "test": "ulimit -S -n 512; node_modules/karma/bin/karma start --single-run",
    "testcontinuous": "ulimit -S -n 512; node_modules/karma/bin/karma start"
  },
  "engineStrict": true,
  "private": true
}
